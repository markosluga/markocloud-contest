{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "pickwinnerrole",
            "Effect": "Allow",
            "Action": [
                "dynamodb:GetItem",
                "dynamodb:Scan",
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:dynamodb:us-west-2:866117724370:table/markocloud-contest",
                "arn:aws:logs:us-west-2:866117724370:*",
                "arn:aws:logs:us-west-2:866117724370:log-group:/aws/lambda/markocloud-contest:*"
            ]
        }
    ]
}
